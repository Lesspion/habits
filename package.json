    "babel-runtime": "^6.23.0",
    "boom": "^4.3.1",
    "hapi": "^16.1.1",
    "hapi-decorators": "^0.4.3",
    "inert": "^4.2.0",
    "iridium": "^7.2.0",
    "joi": "^10.4.2",
    "lodash": "^4.17.4",
