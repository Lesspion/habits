  "scripts": {
    "clean": "rimraf dist habits",
    "setup": "yarn && typings install",
    "build:njk": "cpx \"app/**/*.njk\" dist",
    "build:ts": "tsc",
    "build:js": "webpack --config webpack.config.js",
    "build:sass": "node-sass -r app/public/sass/ -o dist/public/css/",
    "build:img": "imagemin app/public/image/* --out-dir=dist/public/image/",
    "build:watch": "nodemon -e ts --watch app --exec \"yarn run build && yarn run start\"",
    "build:start": "yarn run build && yarn run start",
    "build": "yarn run clean && yarn run build:ts && yarn run build:sass && yarn run build:img && yarn run build:njk",
    "start": "env-cmd ./.env node dist/index.js"
  },
  "devDependencies": {
    "@types/boom": "^4.3.2",
    "@types/hapi": "^16.1.0",
    "@types/joi": "^10.3.2",
    "@types/lodash": "^4.14.64",
    "@types/mongodb": "^2.2.2",
    "@types/node": "^7.0.18",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cpx": "^1.5.0",
    "env-cmd": "^5.1.0",
    "imagemin-cli": "^3.0.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "rifraf": "^2.0.3",
    "typescript": "^2.3.2",
    "webpack": "^2.6.1",
    "yarn": "^0.24.5"
  },
